{
	"extends": "tslint-config-standard",
	"rules": {
		"semicolon": [
			true,
			"always"
		],
		"indent": [
			true,
			"tabs",
			4
		],
		"ter-indent": [
			true, 4,
			{
				"FunctionDeclaration": {
					"parameters": 1,
					"body": 1
				},
				"FunctionExpression": {
					"parameters": 1,
					"body": 1
				},
				"MemberExpression": 1
			}
		]
	}
}
